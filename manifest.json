{
    "manifest_version": 3,
    "name": "Save Website",
    "version": "1.0",
    "description": "Saves website URLs to a sensitive list in localStorage.",
    "permissions": ["activeTab", "storage", "scripting", "tabs"],
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "*://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
  }
  